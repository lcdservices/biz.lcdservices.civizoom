<?xml version="1.0"?>
<extension key="biz.lcdservices.civizoom" type="module">
  <file>civizoom</file>
  <name>CiviZoom</name>
  <description>Zoom integration with CiviCRM Events</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Lighthouse Consulting and Design, Inc.</author>
    <email>dev@lcdservices.biz</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/lcdservices/biz.lcdservices.civizoom</url>
    <url desc="Documentation">https://github.com/lcdservices/biz.lcdservices.civizoom</url>
    <url desc="Support">https://github.com/lcdservices/biz.lcdservices.civizoom/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-09-04</releaseDate>
  <version>3.3</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.60</ver>
  </compatibility>
  <comments/>
  <comments/>
  <civix>
    <namespace>CRM/Civizoom</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_Civizoom_Upgrader</upgrader>
</extension>
